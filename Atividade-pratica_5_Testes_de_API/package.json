		"dotenv": "^16.4.7",
		"express": "^4.21.2",
		"jest": "^29.7.0",
		"jest-mock-extended": "^3.0.7",
		"jsonwebtoken": "^9.0.2",
		"supertest": "^7.1.1",
		"swagger-ui-express": "^5.0.1"
	},
	"devDependencies": {
		"@types/bcrypt": "^5.0.2",
		"@types/cors": "^2.8.17",
